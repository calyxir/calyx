import "primitives/std.lib";
component main() -> () {
  cells {
    a = prim std_reg(32);
    b = prim std_reg(32);
    const0 = prim std_const(32,10);
    const1 = prim std_const(32,1);
    const2 = prim std_const(32,5);
    const3 = prim std_const(32,20);
    const4 = prim std_const(32,40);
    gt0 = prim std_gt(32);
    y = prim std_reg(32);
    z = prim std_reg(32);
  }
  wires {
    group cond0 {
      cond0[done] = 1'd1;
      gt0.left = a.out;
      gt0.right = const2.out;
    }
    group let0 {
      a.in = const0.out;
      a.write_en = 1'd1;
      let0[done] = a.done;
    }
    group let1 {
      b.in = const1.out;
      b.write_en = 1'd1;
      let1[done] = b.done;
    }
    group let2 {
      y.in = const3.out;
      y.write_en = 1'd1;
      let2[done] = y.done;
    }
    group let3 {
      z.in = const4.out;
      z.write_en = 1'd1;
      let3[done] = z.done;
    }
  }
  control {
    seq {
      seq {
        let0;
        let1;
      }
      if gt0.out with cond0 {
        let2;
      } else {
        let3;
      }
    }
  }
}
