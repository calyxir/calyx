import "primitives/core.futil";
component main(@go go: 1, @clk clk: 1, @reset reset: 1) -> (@done done: 1) {
  cells {
    a = std_reg(2);
    b = std_reg(2);
    c = std_reg(2);
    d = std_reg(2);
  }
  wires {
    static group static_seq<4><"NODE_ID"=0> {
      a.in = %[0:2] ? 2'd0;
      a.write_en = %0 & %[0:2] ? 1'd1;
      b.in = %[0:2] ? 2'd1;
      b.write_en = %1 & %[0:2] ? 1'd1;
      c.in = %2 ? 2'd2;
      c.write_en = %2 ? 1'd1;
      d.in = %3 ? 2'd2;
      d.write_en = %3 ? 1'd1;
    }
  }

  control {
    static_seq;
  }
}
