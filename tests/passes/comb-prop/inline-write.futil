// -p well-formed -p comb-prop
import "primitives/core.futil";
import "primitives/memories/comb.futil";
component main() -> () {
  cells {
    w1 = std_wire(32);
    w2 = std_wire(32);
    @external r = std_reg(32);
    @external p = std_reg(32);
  }
  wires {
    group write_p {
      w1.in = r.out;
      p.write_en = 1'd1;
      write_p[done] = p.done;
    }
    w2.in = w1.out;
    p.in = w2.out;
  }
  control {
    write_p;
  }
}
