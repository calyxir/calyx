// -p metadata-table-generation -p uniquefy-enables -x uniquefy-enables:par-thread-json=<err> -x uniquefy-enables:path-descriptor-json=<err>

import "primitives/core.futil";
import "primitives/memories/comb.futil";

component main(@go go: 1) -> (@done done: 1) {
  cells {
    a_reg = std_reg(32);
    b_reg = std_reg(32);
    d_reg = std_reg(32);
    e_reg = std_reg(32);
    lt = std_lt(32);
  }
  wires {
    comb group cond {
      lt.left = a_reg.out;
      lt.right = 32'd8;
    }

    group a {
      a_reg.write_en = 1'd1;
      a_reg.in = 32'd4;
      a[done] = a_reg.done;
    }

    group b {
      b_reg.write_en = 1'd1;
      b_reg.in = 32'd4;
      b[done] = b_reg.done;
    }

    group d {
      d_reg.write_en = 1'd1;
      d_reg.in = 32'd4;
      d[done] = d_reg.done;
    }

    group e {
      e_reg.write_en = 1'd1;
      e_reg.in = 32'd4;
      e[done] = e_reg.done;
    }

  }
  control {
    seq {
      a;
      b;
      par {
	while lt.out with cond {
	  seq {
	   d;
	   e;
	  }
	}
	b;
      }
    }
  }
}
