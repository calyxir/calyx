import "primitives/std.lib";
component main(@go(1) go: 1, @clk(1) clk: 1, @reset(1) reset: 1) -> (@done(1) done: 1) {
  cells {
    add = std_add(32);
  }
  wires {
    group do_add {
      add.right = do_add[go] ? 32'd4;
      add.left = do_add[go] ? 32'd4;
      do_add[done] = 1'd1;
    }
  }

  control {}
}
