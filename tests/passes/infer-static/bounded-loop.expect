import "primitives/core.futil";
component main(@go @static(10) go: 1, @clk clk: 1, @reset reset: 1) -> (@done done: 1) {
  cells {
    @external i = std_mem_d1(32, 1, 1);
    @external j = std_mem_d1(32, 1, 1);
    r = std_reg(1);
    add = std_add(32);
  }
  wires {
    group incr_i<"static"=1> {
      i.write_data = add.out;
      i.addr0 = 1'd0;
      i.write_en = 1'd1;
      add.right = i.read_data;
      add.left = 32'd1;
      incr_i[done] = i.done;
    }
    group incr_j<"static"=1> {
      j.write_data = add.out;
      j.addr0 = 1'd0;
      j.write_en = 1'd1;
      add.right = j.read_data;
      add.left = 32'd1;
      incr_j[done] = j.done;
    }
  }

  control {
    @bound(5) @static(10) while r.out {
      @static(2) seq {
        @static incr_i;
        @static incr_j;
      }
    }
  }
}
