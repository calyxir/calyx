// -p compile-invoke
import "primitives/std.lib";

component exponent(base: 32, exp: 4) -> (out: 32) {
  cells { pow = prim std_reg(32); }
  wires {
    out = pow.out;
  }
  control { }
}

component main() -> () {
  cells {
    r = prim std_reg(32);
    const3 = prim std_const(4,3);
    exp0 = exponent;
  }
  wires {
  }
  control {
    invoke exp0(base = r.out, exp = const3.out)();
  }
}
