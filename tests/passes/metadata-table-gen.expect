import "primitives/core.futil";
component main<"pos"={1}>(@go go: 1, @clk clk: 1, @reset reset: 1) -> (@done done: 1) {
  cells {
    reg1 = std_reg(32);
    reg2 = std_reg(32);
    reg3 = std_reg(32);
    lt = std_lt(32);
  }
  wires {
    group g1<"pos"={2}> {
      reg1.in = 32'd1;
      reg1.write_en = 1'd1;
      done = reg1.done;
    }
    group g2<"pos"={3}> {
      reg2.in = 32'd2;
      reg2.write_en = 1'd1;
      done = reg2.done;
    }
    group g3<"pos"={4}> {
      reg3.in = 32'd3;
      reg3.write_en = 1'd1;
      done = reg3.done;
    }
    group g4<"pos"={5}> {
      reg3.in = 32'd0;
      reg3.write_en = 1'd1;
      done = reg3.done;
    }
    comb group cond {
      lt.left = 32'd5;
      lt.right = 32'd9;
    }
  }
  control {
    @pos{6} par {
      @pos{7} seq {
        @pos{8} g1;
        @pos{9} g2;
      }
      @pos{10} if lt.out with cond {
        @pos{11} g3;
      } else {
        @pos{12} g4;
      }
    }
  }
}
