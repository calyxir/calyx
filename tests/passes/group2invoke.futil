// -p group2invoke -p dead-group-removal
import "primitives/core.futil";
component main() -> () {
  cells {
    r = std_reg(32);
    add = std_add(32);
    lt = std_lt(32);
  }
  wires {
    group write_r {
      r.in = 32'd1;
      r.write_en = 1'd1;
      write_r[done] = r.done;
    }
    group add_write_r{
      r.in = add.out; 
      r.write_en = 1'd1; 
      write_r[done] = r.done; 
      add.left = 32'd2; 
      add.right = 32'd3; 
    }
    group exp{
      lt.left = 32'd5; 
      lt.right = 32'd4; 
      r.write_en = 1'd1; 
      r.in = lt.out ? 32'd2; 
      exp[done] = r.done; 
    }
    group guard2{
      lt.left = 32'd5; 
      lt.right = 32'd4;
      add.left = 32'd2; 
      add.right = lt.out ? r.out; 
      r.write_en = 1'd1; 
      r.in = 32'd2; 
      guard2[done] = r.done; 
    }
  }
  control {
    seq{
      write_r;
      add_write_r;
      exp;
      guard2;
    }
  }
}
