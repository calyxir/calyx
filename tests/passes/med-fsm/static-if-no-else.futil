// -p med-fsm -x tdcc:infer-fsms

import "primitives/core.futil";
import "primitives/memories/comb.futil";

component main() -> () {
  cells {
    a = std_reg(2);
    cond = std_reg(1);
  }

  wires {
    static<1> group A {
      a.in = 2'd0;
      a.write_en = %0 ? 1'd1;
    }
  }

  control {
    static if cond.out {
      A;
    }
  }
}