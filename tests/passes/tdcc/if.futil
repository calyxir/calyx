// -x tdcc:dump-fsm -d static-promotion -d post-opt -d group2invoke -d lower -b none

import "primitives/core.futil";
import "primitives/memories/comb.futil";

component main() -> () {
  cells {
    t = std_reg(1);
    f = std_reg(1);
    lt = std_lt(1);
  }

  wires {
    group true {
      t.in = 1'b1;
      t.write_en = 1'b1;
      true[done] = t.done;
    }

    group false {
      f.in = 1'b1;
      f.write_en = 1'b1;
      false[done] = f.done;
    }

    comb group cond {
      lt.left = 1'b1;
      lt.right = 1'b0;
    }
  }

  control {
    if lt.out with cond {
      true;
    } else {
      false;
    }
  }
}
