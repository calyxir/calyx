import "primitives/core.futil";
component main(@go go: 1, @clk clk: 1, @reset reset: 1) -> (@done done: 1) {
  cells {
    x = std_reg(32);
    y = std_reg(32);
  }
  wires {
    group wr_x {
      x.in = 32'd1;
      x.write_en = 1'd1;
      wr_x[done] = x.done;
    }
    group wr_y {
      y.in = 32'd2;
      y.write_en = 1'd1;
      wr_y[done] = y.done;
    }
  }

  control {
    @NODE_ID(0) par {
      @NODE_ID wr_x;
      @NODE_ID(2) wr_y;
    }
  }
}
