import "primitives/core.futil";
component main(@go go: 1, @clk clk: 1, @reset reset: 1) -> (@done done: 1) {
  cells {
    lt = std_lt(32);
    a = std_reg(32);
    b = std_reg(32);
    c = std_reg(16);
    d = std_reg(16);
    x = std_reg(4);
    sl = std_slice(16, 4);
  }
  wires {
    static group wr_a<1> {
      a.write_en = 1'd1;
      a.in = 32'd2;
    }
    static group wr_b<1> {
      a.write_en = 1'd1;
      a.in = 32'd2;
    }
    static group wr_x<1> {
      x.write_en = 1'd1;
      x.in = 4'd2;
    }
    static group wr_c<1> {
      c.write_en = 1'd1;
      c.in = 16'd4;
    }
    static group wr_d<1> {
      d.write_en = 1'd1;
      d.in = 16'd4;
    }
    static group read_c<1> {
      sl.in = c.out;
      x.write_en = 1'd1;
      x.in = sl.out;
    }
  }

  control {
    seq {
      static par <2> {
        static if <1> lt.out {
          wr_a;
        } else {
          wr_b;
        }
        static seq <2> {
          wr_x;
          wr_b;
        }
      }
      static par <3> {
        static seq <3> {
          wr_c;
          wr_x;
          read_c;
        }
        static seq <3> {
          wr_x;
          wr_d;
          wr_x;
        }
      }
    }
  }
}
