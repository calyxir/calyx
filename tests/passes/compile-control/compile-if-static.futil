// -p static-timing

import "primitives/std.lib";

component main() -> () {
  cells {
    t = std_reg(1);
    f = std_reg(1);
    lt = std_lt(1);
  }

  wires {
    group true<"static"=1> {
      t.in = 1'b1;
      t.write_en = 1'b1;
      true[done] = t.done;
    }

    group false<"static"=1> {
      f.in = 1'b1;
      f.write_en = 1'b1;
      false[done] = f.done;
    }

    group cond<"static"=0> {
      lt.left = 1'b1;
      lt.right = 1'b0;
      cond[done] = 1'b1;
    }
  }

  control {
    if lt.out with cond {
      true;
    } else {
      false;
    }
  }
}
