import "primitives/std.lib";
component exponent(base: 32, exp: 4, go: 1, clk: 1) -> (out: 32, done: 1) {
  cells {
    pow = prim std_reg(32);
  }
  wires {
    out = pow.out;
  }

  control {}
}
component main(go: 1, clk: 1) -> (done: 1) {
  cells {
    r = prim std_reg(32);
    const3 = prim std_const(4, 3);
    exp0 = exponent;
  }
  wires {
    group invoke<"static"=0> {
      exp0.base = r.out;
      exp0.exp = const3.out;
      exp0.go = 1'd1;
      invoke[done] = exp0.done;
    }
  }

  control {
    @static(0) invoke;
  }
}
