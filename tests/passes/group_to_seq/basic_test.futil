//-p canonicalize -p group2seq

import "primitives/core.futil";
import "primitives/binary_operators.futil";
component main<"state_share"=1>(in:32) -> () {
  cells {
    add = std_add(32);
    bin_read0_0 = std_reg(32);
    r = std_reg(32);
    A = std_reg(32);
    B = std_reg(32);
    mult_pipe0 = std_mult_pipe(32);
    x_0 = std_reg(32);
  }
  wires {
    group writeAB{
      A.write_en = 1'd1; 
      A.in = 32'd5; 
      B.write_en = 1'd1; 
      B.in = 32'd7; 
      writeAB[done]= A.done & B.done? 1'd1;
    }
    group two {
      A.write_en = !A.done ? 1'd1; 
      A.in = 32'd4; 
      mult_pipe0.go = A.done; 
      mult_pipe0.left = A.out; 
      mult_pipe0.right = 32'd4;
      two[done] = mult_pipe0.done; 
    }
    group chain_plus_adders {
      add.left = 32'd2;
      add.right = 32'd4;
      x_0.write_en = bin_read0_0.done;
      x_0.in = bin_read0_0.out;
      bin_read0_0.in = add.out;
      bin_read0_0.write_en = !bin_read0_0.done? 1'd1;
      chain_plus_adders[done] = x_0.done;
    }

    group write_a{
      A.in = 32'd2; 
      A.write_en = 1'd1; 
      write_a[done] = A.done; 
    }
    group write_bmult{
      B.in = 32'd2; 
      B.write_en = A.done & !B.done ? 1'd1; 
      mult_pipe0.go = B.done; 
      mult_pipe0.left = B.out; 
      mult_pipe0.right = B.out; 
      write_bmult[done] = mult_pipe0.done;
    }
  }
  control {
    seq{
      writeAB;
      two;
      chain_plus_adders;
      par{
        write_a;
        write_bmult;
      }
    }
  }
}