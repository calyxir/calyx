// -p well-formed -p compile-static

import "primitives/core.futil";

component main() -> () {
  cells {
    a = std_reg(2);
    b = std_reg(2);
    c = std_reg(2);
  }

  wires {
    static group A<2>{
      a.in = 2'd0;
      a.write_en = %0 ? 1'd1;
      b.in = 2'd1;
      b.write_en = %1 ? 1'd1;
    }

    static group C<1>{
      c.in = 2'd2;
      c.write_en = %0 ? 1'd1;
    }
  }

  control {
    seq { A; C; }
  }
}