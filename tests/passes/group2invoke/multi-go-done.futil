// -p well-formed -p group2invoke
import "primitives/core.futil";

extern "./multi-go-done.futil" {
  primitive real_mem(
    @static(1) @go(1) read_en: 1,
    @static(2) @go(2) write_en: 1
  ) -> (
    @done(1) read_done: 1,
    @done(2) write_done: 1
  );
}

component main() -> () {
  cells {
    mem = real_mem();
  }
  wires {
    group write_mem {
      mem.write_en = 1'd1;
      write_mem[done] = mem.write_done;
    }

    group read_mem {
      mem.read_en = 1'd1;
      read_mem[done] = mem.read_done;
    }
  }
  control {
    seq { read_mem; write_mem; }
  }
}
