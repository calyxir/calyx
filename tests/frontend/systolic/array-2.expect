    add = std_add(32);
    mul = std_mult_pipe(32);
  }
  wires {
    group do_add {
      add.left = acc.out;
      add.right = mul.out;
      acc.in = add.out;
      acc.write_en = 1'd1;
      do_add[done] = acc.done;
    }
    out = acc.out;
  }
  control {
    seq {
        invoke mul(left = top, right = left)();
        do_add;
    }
  }
