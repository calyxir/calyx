import "primitives/core.futil";
import "primitives/memories/comb.futil";

component main() -> () {
  cells {
    @external a = comb_mem_d1(32, 3, 32);
    @external b = comb_mem_d1(32, 3, 32);
    @external c = comb_mem_d1(32, 3, 32);
  }

  wires {
    group wr_a {
      a.addr0 = 32'd0;
      a.write_en = 1'b1;
      a.write_data = 32'd1;
      wr_a[done] = a.done;
    }

    group wr_b {
      b.addr0 = 32'd1;
      b.write_en = 1'b1;
      b.write_data = 32'd1;
      wr_b[done] = b.done;
    }

    group wr_c {
      c.addr0 = 32'd2;
      c.write_en = 1'b1;
      c.write_data = 32'd1;
      wr_c[done] = c.done;
    }
  }

  control {
    par {
      wr_a;
      wr_b;
      wr_c;
    }
  }
}
