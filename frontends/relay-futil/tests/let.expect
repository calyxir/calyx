import "primitives/std.lib";
component main() -> () {
  cells {
    x = prim std_reg(32);
    ret0 = prim std_reg(32);
    add1 = prim std_add(32);
    b0 = prim std_reg(32);
    const0 = prim std_const(32, 42);
    add0 = prim std_add(32);
    a0 = prim std_reg(32);
  }
  wires {
    group group2 {
      add1.left = a.out;
      add1.right = b.out;
      ret0.in = add1.out;
      ret0.write_en = 1'd1;
      group2[done] = ret0.done;
    }
    group group1 {
      b0.in = x.out;
      b0.write_en = 1'd1;
      group1[done] = b0.done;
    }
    group group0 {
      a0.in = add0.out;
      a0.write_en = 1'd1;
      group0[done] = a0.done;
      add0.left = x.out;
      add0.right = const0.out;
    }
  }
  control {
    seq {
      group1;
      group0;
      group2;
    }
  }
}
