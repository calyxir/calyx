import "primitives/std.lib";
component main() -> () {
  cells {
    x = prim std_reg(32);
    ret = prim std_reg(32);
    add1 = prim std_add(32);
    v2 = prim std_reg(32);
    const0 = prim std_const(32, 42);
    add0 = prim std_add(32);
    v1 = prim std_reg(32);
  }
  wires {
    group group2 {
      add1.left = v1.out;
      add1.right = v2.out;
      ret.in = add1.out;
      ret.write_en = 1'd1;
      group2[done] = ret.done;
    }
    group group1 {
      v2.in = x.out;
      v2.write_en = 1'd1;
      group1[done] = v2.done;
    }
    group group0 {
      v1.in = add0.out;
      v1.write_en = 1'd1;
      group0[done] = v1.done;
      add0.left = x.out;
      add0.right = const0.out;
    }
  }
  control {
    seq {
      group1;
      group0;
      group2;
    }
  }
}
