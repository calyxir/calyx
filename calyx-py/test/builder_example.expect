import "primitives/core.futil";
import "primitives/binary_operators.futil";
component adder(val1: 32, val2: 32) -> (out: 32) {
  cells {
    sum = std_reg(32);
    add = std_add(32);
  }
  wires {
    group compute_sum {
      add.left = val1;
      add.right = val2;
      sum.write_en = 1'd1;
      sum.in = add.out;
      compute_sum[done] = sum.done;
    }
    out = sum.out;
  }
  control {
    compute_sum;
  }
}
