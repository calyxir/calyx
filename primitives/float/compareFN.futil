extern "compareFN.sv" {
  primitive std_compareFN[
    expWidth,
    sigWidth,
    numWidth
  ](
    @clk clk: 1,
    @reset reset: 1,
    @go go: 1,
    @write_together(1) left: numWidth,
    @write_together(1) right: numWidth,
    signaling: 1
  ) -> (
    lt: 1,
    eq: 1,
    gt: 1,
    unordered: 1,
    exceptionFlags: 5,
    @done done: 1
  );
}
