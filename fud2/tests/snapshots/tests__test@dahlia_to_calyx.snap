---
source: fud2/tests/tests.rs
description: "emit request: dahlia -> calyx"
---
build-tool = fud2
rule get-rsrc
  command = $build-tool get-rsrc $out

parent-map = parent-map.json
dahlia-exe = /test/bin/dahlia
dahlia-args = 
rule dahlia-to-calyx
  command = $dahlia-exe -b calyx --lower -l error $dahlia-args $in > $out

build calyx_1.futil: dahlia-to-calyx dahlia_0.fuse

default calyx_1.futil
