---
source: fud2/tests/tests.rs
description: emit mrxl -> calyx
---
build-tool = fud2
rule get-rsrc
  command = $build-tool get-rsrc $out

# MrXL compiler
mrxl-exe = mrxl
rule mrxl-to-calyx
  command = $mrxl-exe $in > $out

# build targets
build _from_stdin_calyx.futil: mrxl-to-calyx _from_stdin_mrxl.mrxl

default _from_stdin_calyx.futil
