import "primitives/std.lib";

component main() -> () {
  cells {
    const0 = std_const(32, 10);
    const1 = std_const(32, 10);
    gt0 = std_gt(32);
    reg0 = std_reg(1);

  }

  wires {

    group op {
      reg0.write_en = 1'd1;
      gt0.left = const0.out;
      gt0.right = const1.out;
      reg0.in = gt0.out;
      op[done] = reg0.done;
    }
    
  }

//b/c one group, and reg0 decides
//when that group's execution is done, 
//the ending snapshot should include 
//all values on reg0's ports. That is, 
//[done] = 1
  control {
    seq {
      op;
    }
  }
}