import "primitives/std.lib";

component main() -> () {
  cells {
    const0 = std_const(32, 400);
    const1 = std_const(32, 9);
    div0 = std_div(32);
    reg0 = std_reg(32);

  }

  wires {

    group op {
      reg0.write_en = 1'd1;
      div0.left = const0.out;
      div0.right = const1.out;
      reg0.in = div0.out;
      op[done] = reg0.done;
    }
    
  }

  control {
    seq {
      op;
    }
  }
}