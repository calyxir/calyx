import "primitives/std.lib";

component main() -> () {
  cells {
    const0 = prim std_const(32, 9);
    const1 = prim std_const(32, 400);
    add0 = prim std_add(32);
    reg0 = prim std_reg(32);

  }

  wires {

    group op {
      reg0.write_en = 1'd1;
      add0.left = const0.out;
      add0.right = const1.out;
      reg0.in = add0.out;
      op[done] = reg0.done;
    }
    
  }

  control {
    seq {
      op;
    }
  }
}